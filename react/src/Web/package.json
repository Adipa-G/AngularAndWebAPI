{
  "name": "webapi-react",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@types/enzyme": "3.1.13",
    "@types/jquery": "^3.3.6",
    "@types/js-cookie": "2.1.0",
    "@types/react": "^16.4.9",
    "@types/react-dom": "^16.0.7",
    "@types/react-router": "^4.0.30",
    "@types/react-router-dom": "^4.3.0",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1"
  },
  "scripts": {
    "start": "webpack-dev-server --mode=development --hot --inline",
    "build": "webpack --mode=development",
    "test": "jest",
    "preinstall__": "start npm i -g webpack webpack-cli jest && start npm link webpack"
  },
  "devDependencies": {
    "@types/jest": "^23.3.1",
    "awesome-typescript-loader": "^5.2.0",
    "babel-jest": "^23.4.2",
    "enzyme": "^3.4.1",
    "enzyme-adapter-react-16": "^1.2.0",
    "gulp": "^3.9.1",
    "gulp-clean-css": "^2.0.9",
    "gulp-concat": "^2.6.0",
    "gulp-sass": "^3.1.0",
    "gulp-uglify": "^1.5.3",
    "gulp-watch": "^4.3.5",
    "jest": "^23.5.0",
    "jest-cli": "^23.5.0",
    "source-map-loader": "^0.2.3",
    "ts-jest": "^23.1.3",
    "typescript": "^3.0.1",
    "webpack": "4.16.5",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.5"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "setupTestFrameworkScriptFile": "<rootDir>/web-src/app-test/setup.js",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "setup.js"
    ],
    "testRegex": "(/web-src/app-test/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
