{
  "compilerOptions": {
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "module": "system",
    "moduleResolution": "node",
    "noEmitOnError": true,
    "noImplicitAny": false,
    "outDir": "../wwwroot/appScripts/",
    "removeComments": false,
    "sourceMap": false,
    "target": "es6",
    "outFile" : "../wwwroot/appScripts/app.js"
  },
  "exclude": [
    "node_modules"
  ]
}